
{
	"globals": {
	    "jQuery": false,
	    "$": true
	},
	"extends": ["eslint:recommended"],
	"env": {
	    "browser": true,
	    "es6": true,
	    "node": true
	},
	"ecmaFeatures": {
	    "arrowFunctions": true,
	    "modules": true
	},
	"parserOptions": {
	    "ecmaVersion": 6,
	    "sourceType": "module"
	},
	"rules": {
	    "eqeqeq": 1,
		"no-unused-vars": ["warn", { "vars": "local", "args": "after-used" }],
	    "curly": 2,
		"no-console": "off",
	    "quotes": [2, "single"]
	}
}